<p id="notice"><%= notice %></p>

<h1 class="page-title"><%= @review.title %></h1>
<div class="page-image"><%= image_tag @review.picture.url (:medium) %></div>
<div class="review-body"><%= @review.body %></div>
<p>
  <strong>Title:</strong>
  <%= @review.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @review.body %>
</p>

<p>
  <strong>City:</strong>
  <%= @review.city %>
</p>

<%= link_to 'Edit', edit_review_path(@review) %> |
<%= link_to 'Back', reviews_path %>
